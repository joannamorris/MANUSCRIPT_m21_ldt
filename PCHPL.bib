  keywords = {Acoustics,Dyslexia,Human learning,Language,Language acquisition,Learning,Reaction time,Syllables},
  file = {/Users/jmorris6/Zotero/storage/GTJKZANK/Lukács et al. - 2023 - Reading fluency and statistical learning across mo.pdf}
