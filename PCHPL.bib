@article{munteBrainActivityAssociated1997,
  title = {Brain Activity Associated with Syntactic Incongruencies in Words and Pseudo-Words},
  author = {Münte, Thomas F and Matzke, Mike and Johannes, Soenke},
  date = {1997},
  journaltitle = {Journal of Cognitive Neuroscience},
  volume = {9},
  number = {3},
  pages = {318--329},
  abstract = {Event-related potentials (ERPs) were recorded while Ss (aged 20-32 yrs) read either simple sentences made up from real German words, or sentences that contained German pseudo-words instead of nouns and verbs. The verb (pseudo-verb) of the sentences disagreed in number with the subject noun (pseudo-noun) in 50\% of the sentences. The Ss either read the sentences for an interspersed memory test or made a syntactic judgment after each real-word/pseudo-word sentence. While in the real-word condition a late and widespread positivity resembling a previously described syntactic positive shift was found for the disagreeing verbs, a negativity with an onset latency of about 300 msec was seen for the disagreeing pseudo-verbs. In the pseudo-word conditions no positivity followed the initial negativity. This dissociation of negative and positive waves occurring in response to morphosyntactic mismatches by the pseudo/real-word manipulation suggests that the positive shift is a concomitant of a recomputation routine initiated to account for the number incongruency. This routine is based upon the semantics of the sentence and therefore is not observed in the pseudo-word conditions. The earlier negativity appears to be a more direct index of morphosyntactic incongruency. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(Language),(Phonetic,[2530].,*Evoked,*Evoked Potentials,*Morphology,*Morphology (Language),*Syntax,*Words,*Words (Phonetic Units),\&,Adulthood,Electrophysiology,Electrophysiology [2530].,Human.,Human. Adulthood (18 yrs \& older).,Nouns,older).,Potentials,Sentences,Units),Verbs,yrs},
  annotation = {MIT Press, US Empirical Study. Journal Article. English}
}

@article{munteBrainPotentialsSyntactic1998,
  title = {Brain Potentials and Syntactic Violations Revisited: {{No}} Evidence for Specificity of the Syntactic Positive Shift},
  author = {Münte, Thomas F and Heinze, Hans-Jochen and Matzke, Mike and Wieringa, Bernardina M. and Johannes, Soenke},
  date = {1998},
  journaltitle = {Neuropsychologia},
  volume = {36},
  number = {3},
  pages = {217--226},
  abstract = {One of the current issues in the investigation of language by means of event-related brain potentials (ERPs) is whether there is an ERP effect that can be specifically related to the processing of syntactic information. It has been claimed that a late positivity (P600 or SPS [syntactic positive shift]) occurring to syntactic violations or ambiguities qualifies as such an effect. The present study compared ERPs elicited by morphosyntactic (case inflection errors), semantic, and orthographic (misspelled words) violations in a group of 20-27 yr old German Ss. All 3 types of violations gave rise to late positivities having the characteristics of the previously described P600/SPS. In an earlier time window, however, semantic violations were associated with a centroparietally distributed N400 component, whereas syntactic violations gave rise to a negativity of smaller amplitude that had a frontocentral distribution. In light of the present results, the view that the P600/SPS as a whole reflects specific syntactic processes appears to be untenable and an alternative interpretation is proposed. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(Language),[2560].,[2720].,*Evoked,*Evoked Potentials,*Linguistics,*Morphology,*Morphology (Language),*Orthography,*Semantics,\&,Adulthood,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Language,Linguistics,Male.,older).,Potentials,Psychophysiology,Psychophysiology [2560]. Linguistics \& Language \&,Speech,Young,yrs,yrs).},
  annotation = {Elsevier Science, US Empirical Study. Journal Article. English},
  file = {/Users/jmorris6/Zotero/storage/BX7NJII6/Münte et al.. 1998.pdf}
}

@article{munteDecompositionMorphologicallyComplex1999,
  title = {Decomposition of Morphologically Complex Words in {{English}}: {{Evidence}} from Event-Related Brain Potentials},
  author = {Münte, Thomas F and Say, Tessa and Clahsen, Harald and Schiltz, Kolja and Kutas, Marta},
  date = {1999},
  journaltitle = {Cognitive Brain Research},
  volume = {7},
  number = {3},
  pages = {241--253},
  abstract = {Assessed electrophysiological effects of English past tense forms, using a repetition priming paradigm. Event-related brain potentials (ERPs) were recorded from 59 scalp sites as 19 Ss (aged 18-28 yrs) read stem forms of regular and irregular verbs from a list of 1152 words; the stem forms were either preceded (5-9 intervening items) by their past tense forms (primed condition) or by past tense forms of unrelated verbs (unprimed condition). The difference between the ERPs to the primed and unprimed stems was taken as a measure of morphological priming. The ERPs to regular verbs were clearly different from those to irregular verbs: the former were associated with an N400 reduction in the primed condition; primed irregular verb stems, however, showed no such effect. Control conditions demonstrated that the N400 modulation for regular verbs cannot be attributed to formal (i.e., phonological or orthographical) priming. These ERP effects indicate that regular verbs serve as more powerful primes for their corresponding stem forms than irregular past tense forms, suggesting that regular (but not irregular) past tense forms may be decomposed into stem plus affix. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(Language),[2530].,*Evoked,*Evoked Potentials,*Morphology,*Morphology (Language),*Phonology,*Semantic,*Semantic Priming,\&,Adulthood,Electrophysiology,Electrophysiology [2530].,Human.,Human. Adulthood (18 yrs \& older). Young Adulthood,older).,Potentials,Priming,Young,yrs,yrs).},
  annotation = {Elsevier Science Publishers BV, US Empirical Study. Journal Article. English}
}

@article{nagyModulationEventrelatedPotentials1989,
  title = {Modulation of Event-Related Potentials by Word Repetition: {{The}} Effects of Inter-Item Lag},
  author = {Nagy, Margaret E. and Rugg, Michael D.},
  date = {1989},
  journaltitle = {Psychophysiology},
  volume = {26},
  number = {4},
  pages = {431--436},
  abstract = {Examined the modulation of event-related potentials (ERPs) by word repetition (WR) in 12 adults who responded to occasional nonwords interspersed among a series of words. Words were repeated after 0 or 6 intervening items in Exp 1 and after 6 of 19 items in Exp 2. ERPs to WR were characterized by a sustained, widespread positive shift with an onset of 300 msec. The modulation of ERPs by WR is influenced by at least 2 processes; one process acts early during the processing of WR, but subsides rapidly as interitem lag between 1st and 2nd presentations increases. The 2nd process occurs later, but is more robust over variations in inter-item lag. (PsycINFO Database Record (c) 2006 APA, all rights reserved).},
