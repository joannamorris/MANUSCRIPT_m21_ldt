  volume = {112},
  issn = {0027-8424, 1091-6490},
  shorttitle = {Universal Brain Signature of Proficient Reading},
  abstract = {We propose and test a theoretical perspective in which a universal hallmark of successful literacy acquisition is the convergence of the speech and orthographic processing systems onto a common network of neural structures, regardless of how spoken words are represented orthographically in a writing system. During functional MRI, skilled adult readers of four distinct and highly contrasting languages, Spanish, English, Hebrew, and Chinese, performed an identical semantic categorization task to spoken and written words. Results from three complementary analytic approaches demonstrate limited language variation, with speech\textendash{}print convergence emerging as a common brain signature of reading proficiency across the wide spectrum of selected languages, whether their writing system is alphabetic or logographic, whether it is opaque or transparent, and regardless of the phonological and morphological structure it represents.},
  language = {en},
  number = {50},
  journal = {Proceedings of the National Academy of Sciences},
  doi = {10.1073/pnas.1509321112},
  author = {Rueckl, Jay G. and {Paz-Alonso}, Pedro M. and Molfese, Peter J. and Kuo, Wen-Jui and Bick, Atira and Frost, Stephen J. and Hancock, Roeland and Wu, Denise H. and Mencl, William Einar and Du{\~n}abeitia, Jon Andoni and Lee, Jun-Ren and Oliver, Myriam and Zevin, Jason D. and Hoeft, Fumiko and Carreiras, Manuel and Tzeng, Ovid J. L. and Pugh, Kenneth R. and Frost, Ram},
  month = dec,
