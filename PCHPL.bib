@article{eggumEmotionUnderstandingTheory2011,
  title = {Emotion Understanding, Theory of Mind, and Prosocial Orientation: {{Relations}} over Time in Early Childhood},
  shorttitle = {Emotion Understanding, Theory of Mind, and Prosocial Orientation},
  author = {Eggum, Natalie D. and Eisenberg, Nancy and Kao, Karen and Spinrad, Tracy L. and Bolnick, Rebecca and Hofer, Claire and Kupfer, Anne S. and Fabricius, William V.},
  date = {2011-01-01},
  journaltitle = {The Journal of Positive Psychology},
