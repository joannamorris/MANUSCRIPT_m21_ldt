@article{bradleyInfluenceLinguisticSocial2020a,
  title = {The Influence of Linguistic and Social Attitudes on Grammaticality Judgments of Singular ‘They’},
  author = {Bradley, Evan D.},
  date = {2020-03-01},
  journaltitle = {Language Sciences},
  shortjournal = {Language Sciences},
