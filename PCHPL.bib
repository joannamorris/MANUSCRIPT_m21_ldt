}

@article{bickImagingImplicitMorphological2010a,
  title = {Imaging Implicit Morphological Processing: {{Evidence}} from {{Hebrew}}.},
  author = {Bick, Atira S. and Frost, Ram and Goelman, Gadi},
  date = {2010-09},
  journaltitle = {Journal of Cognitive Neuroscience},
  shortjournal = {Journal of Cognitive Neuroscience},
  volume = {22},
  number = {9},
  pages = {1955--1969},
  issn = {1530-8898},
  url = {http://proxy2.hampshire.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&AuthType=ip&db=psyh&AN=2010-18874-007},
  abstract = {Is morphology a discrete and independent element of lexical structure or does it simply reflect a fine-tuning of the system to the statistical correlation that exists among orthographic and semantic properties of words? Hebrew provides a unique opportunity to examine morphological processing in the brain because of its rich morphological system. In an fMRI masked priming experiment, we investigated the neural networks involved in implicit morphological processing in Hebrew. In the 1MFG and 1IFG, activation was found to be significantly reduced when the primes were morphologically related to the targets. This effect was not influenced by the semantic transparency of the morphological prime, and was not found in the semantic or orthographic condition. Additional morphologically related decrease in activation was found in the 1IPL, where activation was significantly modulated by semantic transparency. Our findings regarding implicit morphological processing suggest that morphology is an automatic and distinct aspect of visually processing words. These results also coincide with the behavioral data previously obtained demonstrating the central role of morphological processing in reading Hebrew. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (journal abstract)},
  keywords = {functional magnetic resonance imaging,Lexical Access,lexical structure,morphological processing,Morphology,neural networks,Neurolinguistics,Semantics}
}

@article{bickNeuralCorrelatesMorphological2008a,
  title = {Neural Correlates of Morphological Processes in {{Hebrew}}. [{{References}}]},
  author = {Bick, Atira and Goelman, Gadi and Frost, Ram},
  date = {2008},
  journaltitle = {Journal of Cognitive Neuroscience. Vol},
  volume = {20},
  number = {3},
  pages = {406--420},
  abstract = {Is morphology a discrete and independent element of lexical structure or does it simply reflect a fine tuning of the system to the statistical correlation that exists among the orthographic and semantic properties of words? Imaging studies in English failed to show unequivocal morphological activation that is distinct from semantic or orthographic activation. Cognitive research in Hebrew has revealed that morphological decomposition is an important component of print processing. In Hebrew, morphological relatedness does not necessarily induce a clear semantic relatedness, thus, Hebrew provides a unique opportunity to investigate the neural substrates of morphological processing. In this functional magnetic resonance imaging study, participants were required to perform judgment tasks of morphological relatedness, semantic relatedness, rhyming, and orthographic similarity. Half of the morphologically related words were semantically related and half were semantically unrelated. This design was chosen to induce explicit morphological processing. We identified two locations involved in morphological processing: the left middle frontal gyrus and the left inferior parietal sulcus. Comparing locations of morphological related activation to the locations of semantic and orthographic related activation, we found that the areas neighbored but only partially overlapped. The similarity in activation between the two morphological conditions eliminates the possibility that morphological activation simply results from the semantic properties of the words. These results demonstrate the important role of morphological processing in reading and suggest that morphological analysis is a distinct process of visual word recognition. (PsycINFO Database Record (c) 2008 APA, all rights reserved) (journal abstract).},
  keywords = {(Language),[2520].,*Lexical,*Lexical Access,*Magnetic,*Magnetic Resonance Imaging,*Morphology,*Morphology (Language),*Physiological,*Physiological Correlates,\&,Access,Cinguli,Correlates,Frontal,Frontal Lobe,Gyrus,Gyrus Cinguli,Imaging,Lobe,Neurology,Neuropsychology,Neuropsychology \& Neurology [2520].,Parietal,Parietal Lobe,Resonance,Semantics},
  annotation = {Using Smart Source Parsing Mar}
}

