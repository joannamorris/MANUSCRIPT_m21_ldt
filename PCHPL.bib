@article{ferrandMaskedRepetitionPriming1996,
  title = {The Masked Repetition Priming Effect Dissipates When Increasing the Inter-Stimulus Interval: {{Evidence}} from Word Naming},
  author = {Ferrand, Ludovic},
  date = {1996},
  journaltitle = {Acta Psychologica},
  volume = {91},
  number = {1},
  pages = {15--25},
  abstract = {Investigated masked repetition priming (MRP) across a range of 5 interstimulus intervals (0, 50, 150, 500, and 1000 msec) to test whether there is a (continuous) decrease in any effects to a nonsignificant level; and examined MRP effects for high- and low-frequency words. 20 French-speaking psychology students participated in a word naming experiment using word primes that were either "identical" or "unrelated" to the target word (TW). Ss were to name the TWs as quickly as possible. An ANOVA on the reaction time (RT) data was performed with relatedness, target frequency, and interstimulus intervals as factors. Results clearly show that the MRP effect is a very short-lived phenomenon; while it remains robust for interstimulus intervals up to and including 150 msec, it dissipates with interstimulus intervals of 500 msec and 1000 msec. These suggest that most of the decay of the preactivation of the prime takes place within the 1st 500 msec. (PsycINFO Database Record (c) 2006 APA, all rights reserved).},
