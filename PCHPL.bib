@article{frostStrongPhonologicalTheory1998,
  title = {Toward a Strong Phonological Theory of Visual Word Recognition: True Issues and False Trails},
  shorttitle = {Toward a Strong Phonological Theory of Visual Word Recognition},
  author = {Frost, R.},
  date = {1998-01},
