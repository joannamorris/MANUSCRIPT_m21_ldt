  annotation = {References . Taylor \& Francis, United Kingdom English Empirical Study Peer Reviewed Journal},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6JVW3FHC/Longtin, Segui, Halle. 2003.pdf}
}

@article{lopez-calderonERPLABOpensourceToolbox2014,
  ids = {lopez-calderon_erplab_2014},
  title = {{{ERPLAB}}: An Open-Source Toolbox for the Analysis of Event-Related Potentials},
