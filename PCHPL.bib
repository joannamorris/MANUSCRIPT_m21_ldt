@article{feldmanMorphologicalAnalysisChild2002,
  title = {Morphological Analysis by Child Readers as Revealed by the Fragment Completion Task},
  author = {Feldman, Laurie Beth and Rueckl, Jay and DiLiberto, Kristen and Pastizzo, Matthew John and Vellutino, Frank R.},
  date = {2002},
  journaltitle = {Psychonomic Bulletin \& Review},
  volume = {9},
  number = {3},
  pages = {529--535},
  abstract = {88 10-yr-old children performed a fragment completion task. Target fragments (e.g., T\_ \_N) were preceded by 4 types of study conditions. The identity condition consisted of the target (TURN). The morphological condition included a related form (TURNED). The orthographic condition consisted of morphologically unrelated words (TURNIP). Finally, no similar word was presented in the study phase of the no-prime condition. Morphological relatives included orthographically transparent (TURNED-TURN) and orthographically opaque (RIDDEN-RIDE) forms. The results indicate that performance of child readers on the fragment completion task was sensitive to morphological relationships. Completion rates following opaque, as well as transparent, morphological relatives were significantly greater than those following orthographically similar forms. In sum, the fragment completion task provides a viable tool for examining morphological processing in children and for differentiating morphological effects from effects of similar form. (PsycINFO Database Record (c) 2003 APA, all rights reserved).},
