@article{kielarGradedEffectsRegularity2009,
  title = {Graded {{Effects}} of {{Regularity}} in {{Language Revealed}} by {{N400 Indices}} of {{Morphological Priming}}},
  author = {Kielar, Aneta and Joanisse, Marc F.},
  date = {2009-10-05},
  journaltitle = {Journal of Cognitive Neuroscience},
