@article{friedmanERPComponentsPicture1988,
  title = {{{ERP Components}} in {{Picture Matching}} in {{Children}} and {{Adults}}},
  author = {Friedman, David and Sutton, Samuel and Putnam, Lois and Brown Jr, Charles and {Erlenmeyer-Kimling}, L.},
