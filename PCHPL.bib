@article{kielarNeuralCorrelatesCovert2011,
  title = {Neural Correlates of Covert and Overt Production of Tense and Agreement Morphology: {{Evidence}} from {{fMRI}}},
  shorttitle = {Neural Correlates of Covert and Overt Production of Tense and Agreement Morphology},
  author = {Kielar, Aneta and Milman, Lisa and Bonakdarpour, Borna and Thompson, Cynthia K.},
  date = {2011-03},
  journaltitle = {Journal of Neurolinguistics},
  shortjournal = {Journal of Neurolinguistics},
  series = {Clinical {{Speech}} and {{Language Studies}} in {{Honour}} of {{Susan Edwards}}},
  volume = {24},
