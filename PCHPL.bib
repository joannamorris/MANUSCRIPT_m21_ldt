@article{daffnerPathophysiologyUnderlyingDiminished2001,
  title = {Pathophysiology Underlying Diminished Attention to Novel Events in Patients with Early {{AD}}},
  author = {Daffner, Kirk R. and Rentz, D. and Scinto, L. and Faust, R. and Budson, A. and Holcomb, P.},
  date = {2001},
  journaltitle = {Neurology},
  volume = {56},
  number = {10},
  pages = {1377--1383},
  abstract = {Patients with mild to moderate AD often are apathetic and fail to attend to novel aspects of their environment. The authors investigated the mechanisms underlying these changes by studying the novelty P3 response that measures shifts of attention toward novel events. While event-related potentials were recorded, 10 mildly impaired AD patients and 20 matched normal controls (NC) viewed line drawings that included a repetitive background stimulus, an infrequent target stimulus, and infrequent novel stimuli. Results showed that P3 amplitude to novel stimuli was significantly smaller for AD patients than NC. For NC, P3 response to novel stimuli was much larger than to background stimuli. Although NC spent more time looking at novel than background stimuli, patients with AD distributed their viewing time evenly. Remarkably, for patients with AD, the amplitude of the novelty P3 response powerfully predicted how long they would spend looking at novel stimuli and inversely correlated with apathy severity. The novelty P3 response is markedly diminished in mild AD, at a time when the target P3 response is preserved. The disruption of the novelty P3 response predicts diminished attention to novel stimuli and is associated with the apathy exhibited by patients with AD. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,[3297].,*Alzheimers,*Alzheimers Disease,*Attention,*Environment,*Stimulus,*Stimulus Novelty,*Visual,*Visual Evoked Potentials,\&,Adulthood,Brain,Damage,Disease,Disorders,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older).,Male.,Neurological,Neurological Disorders \& Brain Damage [3297].,Novelty,older).,Potentials,yrs},
  annotation = {Lippincott Williams \& Wilkins, US Conference Proceedings/Symposia. Empirical Study. Journal Article. English}
}

@article{daffnerRegulationAttentionNovel1998,
