@article{deaconStatisticalLearningPerspective2008,
  title = {A {{Statistical Learning Perspective}} on {{Children}}'s {{Learning About Graphotactic}} and {{Morphological Regularities}} in {{Spelling}}},
  author = {Deacon, S. Hélène and Conrad, Nicole and Pacton, Sébastien},
  date = {2008-05},
  journaltitle = {Canadian Psychology; Ottawa},
  volume = {49},
  number = {2},
  pages = {118--124},
  issn = {07085591},
  url = {https://search.proquest.com/docview/220814613/abstract/2F99E2FC2855436APQ/1},
  urldate = {2019-05-12},
  abstract = {The first point of evidence opposing rule-based learning is the disconnect between the idea of children's initial and exclusive reliance on a phonetic rule in spelling and the growing amount of evidence showing that children young enough to be in this stage demonstrate sensitivity to morphological regularities in their spelling. This is the case for the 5- and 6-year-olds in Treiman and Cassar's (1996) study, and the 6-year-olds in Deacon and [Bryant]'s (2006) study. (Such a concern also exists in relation to the early findings of sensitivity to graphotactic regularities; e.g., [Pacton] et al., 2001). Certainly, some of these discrepancies can be accounted for by the context of assessment. For example, there is some evidence that increasing task demands (e.g., introducing a secondary task or varying the quantity spelt) increases agreement errors for French spellers (e.g., Hupet, Fayol, \& Schelstraete, 1998). For English, a similar pattern emerges based on the metric of analysis. The spelling tund (instead of tuned) is categorized as a phonetic spelling not influenced by morphology by [Nunes] et al. (1997b) who focused on the correct use of -ed, but [Treiman] and Cassar ( 1996) considered such a spelling as influenced by morphology if /n/ was transcribed accurately more often in two- than in one-morpheme words (e.g., tuned vs. brand). More important, not all findings can be discounted on this basis. Deacon and Bryant (2005; 2006) recently demonstrated that young children demonstrate an appreciation of the relationship between morphemes and spelling even though the coding required correct spellings of whole morphemes and not just single letters (see also Deacon, 2008). Young children's appreciation of the role of morphemes in spelling goes directly against the predictions made for Nunes et al.'s stage of initially phonetic spelling. Nous présentons notre point de vue sur la manière dont les enfants apprennent l'orthographe en nous appuyant sur une évaluation de la recherche récente en matière de développement des compétences en lecture et en écriture des enfants francophones et anglophones. Nous nous penchons sur la question de l'apprentissage des codes graphotactiques (combinaisons de lettres admises), après quoi nous examinons plus à fond la manière dont les enfants apprennent la position des morphèmes (les plus petites unités de sens de la langue). Nous mettons en lumière les différences entre les résultats de la recherche récente et les modèles traditionnels voulant que les enfants se servent de l'information graphotactique et morphologique relativement tard dans leur apprentissage de l'orthographe, et selon lesquels le développement des compétences, en particulier la reconnaissance des codes morphologiques, s'appuie essentiellement sur l'apprentissage des règles d'orthographe. En fait, il semble que les enfants en très bas âge soient influencés, dans leur apprentissage de l'orthographe, par les structures graphotactiques comme par les structures morphologiques et qu'en définitive, ils ne dépendent pas exclusivement des règles pour écrire. C'est pourquoi nous formulons la possibilité que les enfants pourraient apprendre à reconnaître les codes graphotactiques et morphologiques de l'orthographe par apprentissage statistique.},
  langid = {english},
  keywords = {Children \& youth,Learning,Psychology,Spelling,Vocabulary development},
  file = {/Users/jmorris6/Zotero/storage/HCA7G47D/Deacon et al. - 2008 - A Statistical Learning Perspective on Children's L.pdf}
}

