}

@article{yapIndividualDifferencesJoint2009,
  title = {Individual Differences in the Joint Effects of Semantic Priming and Word Frequency Revealed by {{RT}} Distributional Analyses: {{The}} Role of Lexical Integrity},
  shorttitle = {Individual Differences in the Joint Effects of Semantic Priming and Word Frequency Revealed by {{RT}} Distributional Analyses},
  author = {Yap, Melvin J. and Tse, Chi-Shing and Balota, David A.},
