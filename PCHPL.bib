}

@article{ferreiraAutomaticControlledComponents2006,
  title = {Automatic and Controlled Components of Judgment and Decision Making},
  author = {Ferreira, Mario B. and Garcia-Marques, Leonel and Sherman, Steven J. and Sherman, Jeffrey W.},
  date = {2006},
  journaltitle = {Journal of Personality and Social Psychology},
  volume = {91},
  number = {5},
  pages = {797--813},
  issn = {1939-1315(Electronic);0022-3514(Print)},
  doi = {10.1037/0022-3514.91.5.797},
  abstract = {The categorization of inductive reasoning into largely automatic processes (heuristic reasoning) and controlled analytical processes (rule-based reasoning) put forward by dual-process approaches of judgment under uncertainty (e.g., K. E. Stanovich \& R. F. West, 2000) has been primarily a matter of assumption with a scarcity of direct empirical findings supporting it. The present authors use the process dissociation procedure (L. L. Jacoby, 1991) to provide convergent evidence validating a dual-process perspective to judgment under uncertainty based on the independent contributions of heuristic and rule-based reasoning. Process dissociations based on experimental manipulation of variables were derived from the most relevant theoretical properties typically used to contrast the two forms of reasoning. These include processing goals (Experiment 1), cognitive resources (Experiment 2), priming (Experiment 3), and formal training (Experiment 4); the results consistently support the author's perspective. They conclude that judgment under uncertainty is neither an automatic nor a controlled process but that it reflects both processes, with each making independent contributions.},
  keywords = {*Cognitive Processes,*Decision Making,*Judgment,*Reasoning,Heuristics,Inductive Deductive Reasoning,Uncertainty}
}

