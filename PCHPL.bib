@article{dacruzAutomaticPreprocessingPipeline2018,
  title = {An Automatic Pre-Processing Pipeline for {{EEG}} Analysis ({{APP}}) Based on Robust Statistics},
  author = {family=Cruz, given=Janir Ramos, prefix=da, useprefix=true and Chicherov, Vitaly and Herzog, Michael H. and Figueiredo, Patrícia},
  date = {2018-07-01},
  journaltitle = {Clinical Neurophysiology},
  shortjournal = {Clinical Neurophysiology},
  volume = {129},
  number = {7},
  pages = {1427--1437},
  issn = {1388-2457},
  doi = {10.1016/j.clinph.2018.04.600},
  url = {https://www.sciencedirect.com/science/article/pii/S1388245718309015},
  urldate = {2023-01-11},
  abstract = {Objective With the advent of high-density EEG and studies of large numbers of participants, yielding increasingly greater amounts of data, supervised methods for artifact rejection have become excessively time consuming. Here, we propose a novel automatic pipeline (APP) for pre-processing and artifact rejection of EEG data, which innovates relative to existing methods by not only following state-of-the-art guidelines but also further employing robust statistics. Methods APP was tested on event-related potential (ERP) data from healthy participants and schizophrenia patients, and resting-state (RS) data from healthy participants. Its performance was compared with that of existing automatic methods (FASTER for ERP data, TAPEEG and Prep pipeline for RS data) and supervised pre-processing by experts. Results APP rejected fewer bad channels and bad epochs than the other methods. In the ERP study, it produced significantly higher amplitudes than FASTER, which were consistent with the supervised scheme. In the RS study, it produced spectral measures that correlated well with the automatic alternatives and the supervised scheme. Conclusion APP effectively removed EEG artifacts, performing similarly to the supervised scheme and outperforming existing automatic alternatives. Significance The proposed automatic pipeline provides a reliable and efficient tool for pre-processing large datasets of both evoked and resting-state EEG.},
  langid = {english},
  keywords = {Automatic pre-processing,Electroencephalography,ERP,Resting-state},
  file = {/Users/jmorris6/Zotero/storage/4HKQGS8F/dacruz_2018.pdf;/Users/jmorris6/Zotero/storage/6Z5RRB6R/da Cruz et al. - 2018 - An automatic pre-processing pipeline for EEG analy.pdf;/Users/jmorris6/Zotero/storage/Q8DEDTTM/S1388245718309015.html}
}

@article{daffnerCentralRolePrefrontal2000,
  title = {The Central Role of the Prefrontal Cortex in Directing Attention to Novel Events},
  author = {Daffner, Kirk R. and Mesulam, M. and Scinto, L. and Acar, D. and Calvo, V. and Raust, R. and Chabrerie, A. and Kennedy, B. and Holcomb, P.},
  date = {2000},
  journaltitle = {Brain},
  volume = {123},
  number = {5},
  pages = {927--939},
  abstract = {Recorded event-related potentials (ERPs) from 9 patients (aged 25-79 yrs) with frontal lobe damage and 20 matched normal controls, who controlled duration of viewing of background, novel, and target stimuli. Patients exhibited markedly reduced amplitude of the novelty P3 response and the duration of viewing of novel stimuli. Injury to the frontal lobes had a limited impact on P3 amplitude an behavioral responses (viewing duration and reaction time [RT]) to target stimuli. A strong correlation was found between measures of apathy and both attenuated amplitude and viewing duration in response to novel, but not target, stimuli. Differences in amplitude of the novelty P3 response explained a large portion of the variance associated with duration of viewing of novel stimuli. After controlling for the influence of P3 amplitude, there was no association between frontal lobe injury and reduced viewing of novel stimuli. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(30-39,(40-64,(65,[3297].,*Attention,*Brain,*Brain Damage,*Frontal,*Frontal Lobe,*Stimulus,*Stimulus Novelty,*Visual,*Visual Evoked Potentials,\&,Adulthood,Age,Aged,Brain,Damage,Disorders,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Lobe,Male.,Middle,Neurological,Neurological Disorders \& Brain Damage [3297].,Novelty,older).,Potentials,Reaction,reaction time,Thirties,Time,Young,yrs,yrs).},
  annotation = {Oxford Univ Press, England Empirical Study. Journal Article. English}
}

@article{daffnerDisruptionAttentionNovel2000,
  title = {Disruption of Attention to Novel Events after Frontal Lobe Injury in Humans},
  author = {Daffner, Kirk R. and Mesulam, M. and Holcomb, Philip J. and Calvo, Vivian and Acar, Diler and Chabrerie, Alexandra and Kikinis, Ron and Jolesz, Ferenc A. and Rentz, Dorene M. and Scinto, Leonard F.},
  date = {2000},
  journaltitle = {Journal of Neurology, Neurosurgery \& Psychiatry},
  volume = {68},
  number = {1},
  pages = {18--24},
  abstract = {Investigated whether frontal lobe damage in humans disrupts the natural tendency to preferentially attend to novel visual events in the environment. Nine patients (mean age 61.2 yrs) with chronic infarctions in the dorsolateral prefrontal cortex and 23 matched normal controls (mean age 68.3 yrs) participated in a study in which Ss viewed repetitive background stimuli, infrequent target stimuli, and novel visual stimuli. Ss controlled viewing duration by a button press that led to the onset of the next stimulus. Ss also responded to targets by pressing a foot pedal. Results show that frontal lobe injury selectively impaired the natural tendency to seek stimulation from novel and unusual stimuli. These data provide the first quantitative behavioral demonstration that the human frontal lobes play a critical part in directing and sustaining attention to novel events. The impairment of novelty seeking behavior may contribute to the characteristic apathy in found in patients with frontal lobe injury. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(40-64,[3297].,*Attention,*Cerebrovascular,*Cerebrovascular Disorders,*Frontal,*Frontal Lobe,*Stimulus,*Stimulus Novelty,*Visual,*Visual Stimulation,\&,Adulthood,Age,Brain,Damage,Disorders,Human.,Human. Adulthood (18 yrs \& older). Middle Age (40-,Lobe,Middle,Neurological,Neurological Disorders \& Brain Damage [3297].,Novelty,older).,Stimulation,yrs,yrs).},
  annotation = {BMJ Publishing Group, England Empirical Study. Journal Article. English}
}

@article{daffnerElectrophysiologicalIndexStimulus2000,
  title = {An Electrophysiological Index of Stimulus Unfamiliarity},
  author = {Daffner, Kirk R. and Mesulam, M. and Scinto, Leonard F. and Calvo, Vivian and Faust, Robert and Holcomb, Phillip J.},
  date = {2000},
  journaltitle = {Psychophysiology},
  volume = {37},
  number = {6},
  pages = {737--747},
  abstract = {Investigated the functional significance of the N2 response to novel stimuli with 24 right-handed college students (mean age 21 yrs). In one condition, background, target, and deviant stimuli were simple geometric figures. In a second condition, all stimulus types were unfamiliar/unusual figures. In a third condition, background and target stimuli were unusual figures and deviant stimuli were simple shapes. Unusual figures, whether they were deviant, target, or background stimuli, evoked larger N2 responses than their simple, familiar counterparts. N2 elicited by an unusual background stimulus was larger than that evoked by simple, deviant stimuli, a pattern opposite that exhibited by the subsequent P3. Deviance from immediate context had limited influence over N2 amplitude. The results suggest that novelty N2 and novelty P3 reflect the processing of different aspects of 'novel' visual stimuli. The novelty P3 is particularly sensitive to deviation from immediate context. In contrast, the novelty N2 is sensitive to deviation from long-term context that renders a stimulus unfamiliar and difficult to encode. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,[2530].,*Familiarity,*Stimulus,*Stimulus Novelty,*Visual,*Visual Evoked Potentials,\&,Adulthood,Electrophysiology,Electrophysiology [2530].,Evoked,Human.,Human. Adulthood (18 yrs \& older). Young Adulthood,Novelty,older).,Potentials,Young,yrs,yrs).},
  annotation = {Cambridge Univ Press, US Empirical Study. Journal Article. English}
}

@article{daffnerFrontalParietalComponents2003,
  title = {Frontal and Parietal Components of a Cerebral Network Mediating Voluntary Attention to Novel Events},
  author = {Daffner, K. and Scinto, L. and Weitzman, A. and Faust, R. and Rentz, D. and Budson, A. and Holcomb, P.},
  date = {2003},
  journaltitle = {Journal of Cognitive Neuroscience},
  volume = {15},
  number = {2},
  pages = {294--313},
  abstract = {Investigated the relative contributions of the frontal and posterior parietal lobes to the differential processing of novel and target stimuli under an experimental condition in which subjects actively directed attention to novel events. Event-related potentials were recorded from well-matched frontal patients, parietal patients, and non-brain-injured subjects who controlled their viewing duration (by button press) of line drawings that included a frequent, repetitive background stimulus, an infrequent target stimulus, and infrequent, novel visual stimuli. Damage to the frontal cortex resulted in a much greater disruption of response to novel stimuli than to designated targets. Frontal patients exhibited a widely distributed, profound reduction of the novelty P3 response and a marked diminution of the viewing duration of novel events. In contrast, damage to posterior parietal lobes was associated with a substantial reduction of both target P3 and novelty P3 amplitude; however, there was less disruption of the processing of novel than of target stimuli. We conclude that two nodes of the neuroanatomical network for responding to and processing novelty are the prefrontal and posterior parietal regions, which participate in the voluntary allocation of attention to novel events. (PsycINFO Database Record (c) 2003 APA, all rights reserved).},
  keywords = {(18,(Events),[2530].,*Attention,*Frontal,*Frontal Lobe,*Parietal,*Parietal Lobe,*Stimulus,*Stimulus Novelty,*Visual,*Visual Evoked Potentials,\&,Adulthood,Attention,Electrophysiology,Electrophysiology [2530].,Evoked,Experiences,Experiences (Events),Human.,Human. Adulthood (18 yrs \& older).,Lobe,Networks,Neural,neural networks,Novelty,older).,Patients,Potentials,Selective,Selective Attention,yrs},
  annotation = {MIT Press, US Empirical Study. Journal Article. English}
}

@article{daffnerInfluenceStimulusDeviance2000,
  title = {The Influence of Stimulus Deviance on Electrophysiologic and Behavioral Responses to Novel Events},
  author = {Daffner, Kirk R. and Scinto, Leonard F. and Calvo, Vivian and Faust, Robert and Mesulam, M. and West, W. and Holcomb, Phillip J.},
  date = {2000},
  journaltitle = {Journal of Cognitive Neuroscience},
  volume = {12},
  number = {3},
  pages = {393--406},
  abstract = {Investigated the role of stimulus deviance in determining electrophysiologic and behavioral responses to "novelty." 24 undergraduates participated in a visual event related potential (ERP) experiment, in which they controlled the duration of stimulus viewing with a button press, which served as a measure of exploratory behavior. Each of the 3 experimental conditions included a frequent repetitive background stimulus and infrequent stimuli that deviated from the background stimulus. Deviant stimuli elicited larger N2-P3 amplitudes and longer viewing durations than the repetitive background stimulus, even when the deviant stimuli were simple, familiar shapes and the background stimulus was a highly unusual figure. Compared with simple, familiar deviant stimuli, unusual deviant stimuli elicited larger N2-P3 amplitudes and longer viewing times. Within Ss, the deviant stimuli that evoked the largest N2-P3 responses also elicited the longest viewing durations. Deviance from both immediate context and long-term prior experience contributes to the response to novelty, with the combination generating the largest N2-P3 amplitude and the most sustained attention. The amplitude of the N2-P3 may reflect how much "uncertainty" is evoked by a novel visual stimulus and signal the need for further exploration. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,[2530].,*Response,*Response Duration,*Stimulus,*Stimulus Novelty,*Visual,*Visual Evoked Potentials,*Visual Stimulation,\&,Adulthood,Duration,Electrophysiology,Electrophysiology [2530].,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older).,Male.,Novelty,older).,Potentials,Stimulation,yrs},
