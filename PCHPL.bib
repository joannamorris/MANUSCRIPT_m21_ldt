  file = {/Users/jmorris6/Zotero/storage/LFBDJ8HZ/Barr et al. - 2013 - Random effects structure for confirmatory hypothes.pdf;/Users/jmorris6/Zotero/storage/VWRCWH7I/S0749596X12001180.html}
