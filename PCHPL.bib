@article{mcphersonEventrelatedBrainPotentials1998,
  title = {Event-Related Brain Potentials Elicited during Phonological Processing Differentiate Subgroups of Reading Disabled Adolescents},
  author = {McPherson, W. and Ackerman, P. and Holcomb, P. and Dykman, R.},
  date = {1998},
  journaltitle = {Brain \& Language},
  volume = {62},
  number = {2},
  pages = {163--185},
  abstract = {Visual and auditory rhyme judgment tasks were administered to 32 adolescent dyslexics and normal readers (aged 13-18 yrs) while event-related brain potentials were recorded. Reading disabled Ss were split into 2 groups based on a median split of scores on a visual non-word decoding test. The better decoders were called Phonetics, and the poorer decoders were referred to as Dysphonetics. Single syllable, real word stimuli were used, and both rhyming and non-rhyming targets had a 50\% chance for matching orthography. In the visual paradigm the normal readers exhibited a left frontal contingent negative variation (CNV) before targets, a large reduction in frontal N400 for matching orthography (orthographic priming), and a large reduction in parietal N400 for rhyming targets (phonological priming). Dysphonetics had an intact CNV and orthographic priming, but the group's phonological priming was very reduced. Phonetics showed both orthographic and phonological priming but had a marked reduction in their CNV. In the auditory task, controls showed a left parietal N400 priming effect for rhyming targets. Dysphonetics showed a similar bilateral effect. The Phonetics did not show a normal priming effect, but produced evidence for priming at a longer latency. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(13-17,(18,[2530].,[3253].,*Auditory,*Auditory Evoked Potentials,*Dyslexia,*Judgment,*Phonology,*Visual,*Visual Evoked Potentials,\&,Adolescence,Adulthood,Disorders,Electrophysiology,Evoked,Human.,Human. Adolescence (13-17 yrs). Adulthood (18 yrs,Learning,Learning Disorders [3253]. Electrophysiology [2530,older).,Potentials,yrs,yrs).},
