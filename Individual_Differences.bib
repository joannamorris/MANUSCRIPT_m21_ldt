@article{havasBrainSignaturesEarly2017,
  title = {Brain Signatures of Early Lexical and Morphological Learning of a New Language},
  author = {Havas, Vikt{\'o}ria and Laine, Matti and Rodr{\'i}guez Fornells, Antoni},
