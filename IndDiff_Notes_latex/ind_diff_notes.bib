@article{hasherTrulyIncidentalEncoding1987,
  title = {Truly {{Incidental Encoding}} of {{Frequency Information}}},
  author = {Hasher, Lynn and Zacks, Rose T. and Rose, Karen C. and Sanft, Henrianne},
  year = {1987},
  journal = {The American Journal of Psychology},
  volume = {100},
  number = {1},
