  journaltitle = {Cognitive Brain Research},
  volume = {13},
  number = {3},
  pages = {363--375},
  abstract = {Examined event-related potentials (ERPs) elicited in response to semantic processing of non-verbal stories. ERPs were recorded from 29 scalp electrodes on 16 subjects (Ss; 11 females; aged 18-21 yrs) while they viewed series of complex gray-scale pictures, each of which relayed a simple story. The final picture of each story was either congruous or incongruous with the preceding context. Ss made delayed meaningfulness judgments for each story. Averaged ERPs time-locked to the onset of the final picture were more negative for incongruous than congruous pictures. Two distinct components were sensitive to congruency. The 1st component peaked at 325 ms (N300) and was distributed over central and frontal sites. The 2nd component peaked at 500 ms and also had a centro-frontal maximum but was more widespread than the earlier component (anterior N400). The distinct scalp topographies of these 2 negativities provide strong evidence that the N300 and N400 are separate and distinguishable components. The presence of the N300 in this exclusively pictorial task suggests that the N300 is specific to the semantic processing of non-verbal stimuli and is not due to linguistic mediation. N400 can be modulated by discourse-level coherence manipulations with pictures. (PsycINFO Database Record (c) 2003 APA, all rights reserved).},
  keywords = {(18,[2530].,*Cognitive,*Cognitive Processes,*Evoked,*Evoked Potentials,*Pictorial,*Pictorial Stimuli,*Semantics,\&,Adulthood,Electrophysiology,Electrophysiology [2530].,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older).,Male.,older).,Potentials,Processes,Stimuli,yrs},
  annotation = {Elsevier Science Publishers BV, US Empirical Study. Journal Article. English},
  file = {/Users/jmorris6/Zotero/storage/2MAK5VLH/6 West.PictureStoryFinal.pdf}
}

@article{westImaginalSemanticSurfacelevel2000,
  title = {Imaginal, Semantic, and Surface-Level Processing of Concrete and Abstract Words: {{An}} Electrophysiological Investigation},
  author = {West, W. and Holcomb, Phillip J.},
  date = {2000},
  journaltitle = {Journal of Cognitive Neuroscience},
  volume = {12},
  number = {6},
  pages = {1024--1037},
  abstract = {Examined concrete and abstract words using both reaction time (RT) and event-related brain potentials (ERP) measurements to determine (1) at what point in the stream of cognitive processing concreteness effects emerge and (2) how different types of cognitive operations influence these concreteness effects. Three groups of Ss ( n = 36; aged 18-23 yrs) performed a sentence verification task in which the final word of each sentence was concrete or abstract. For each group the truthfulness judgment required either image generation, a semantic decision, or evaluation of surface characteristics. Results indicate that concrete and abstract words produced similar RTs and ERPs in the surface task, suggesting that postlexical semantic processing is necessary to elicit concreteness effects. In both the semantic and imagery tasks, RTs were shorter for concrete than for abstract words. This difference was greatest in the imagery task. Also, in both of these tasks concrete words elicited more negative ERPs than abstract words between 300 and 550 msec, an effect that was widespread across the scalp. ERPs were also more negative for concrete than abstract words between 550 and 800 msec. This effect was more frontally distributed and was most evident in the imagery task. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(Phonetic,[2530].,*Cognitive,*Cognitive Processes,*Cortical,*Cortical Evoked Potentials,*Imagery,*Reaction,*Reaction Time,*Words,*Words (Phonetic Units),\&,Adulthood,Decision,Decision Making,Electrophysiology,Electrophysiology [2530].,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Making,Male.,older).,Potentials,Processes,Semantics,Time,Units),Young,yrs,yrs).},
