@article{serviceRecallMorphologicallyComplex2002,
  title = {Recall of Morphologically Complex Forms Is Affected by Memory Task but Not Dyslexia},
  author = {Service, Elisabet and Tujulin, Anna-Mari},
  date = {2002},
  journaltitle = {Brain \& Language},
  volume = {81},
  number = {1-3},
  pages = {42--54},
  abstract = {The authors studied the effect of morphological complexity on working memory in list recall tasks with base words, inflected words and derived words in a morphologically rich language: Finnish. Simple serial recall was compared to complex working memory tasks, combining word recall with sentence verification in normally reading participants (15 boys and 14 girls, aged 7 yrs 11 mo-9 yrs 1 mo), dyslexic children (6 boys and 6 girls, aged 8 yrs 1 mo-9 yrs), and adults (19 Ss, aged 18-46 yrs). The normally reading children performed better than dyslexic children on both memory tasks and a test of morphology. Base words were better recalled than morphologically complex words. Memory was better for derived than inflected words in simple but not complex span tasks. There was no interaction between word type and reading group and thus no suggestion of dyslexia being associated with specific problems to represent complex morphology in working memory. Morphological processing in working memory appeared to depend on the task. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
