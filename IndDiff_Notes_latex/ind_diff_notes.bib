}

@incollection{perfettiLexicalBasisComprehension2001,
  ids = {perfetti_lexical_2001-1},
  title = {The Lexical Basis of Comprehension Skill},
  booktitle = {On the Consequences of Meaning Selection: {{Perspectives}} on Resolving Lexical Ambiguity},
  author = {Perfetti, Charles A. and Hart, Lesley},
  year = {2001},
  series = {Decade of Behavior.},
  pages = {67--86},
  publisher = {{American Psychological Association}},
  address = {{Washington, DC, US}},
  doi = {10.1037/10459-004},
  abstract = {How shall we understand the source of individual variation in comprehension skill? What is it that skilled comprehenders do during reading that less-skilled readers do not do? In this chapter, we revisit this question from a long-range perspective coupled with some new research. C. A. Perfetti (1985) suggested an answer to this question roughly as follows: Although skilled comprehenders differ from less-skilled comprehenders in a number of ways, at least some of these differences arise from a foundational processing factor: the effectiveness of basic word identification skill. Many--not all--problems in comprehension arose from ineffective lower level processes needed for the identification of words. Rapid and perhaps modular word identification was important for a comprehension system of limited capacity. The question we address here is, roughly, does some part of this idea remain tenable? And what more specific instantiation of the general idea is now possible? In what follows, we first review the background of verbal efficiency theory and then illustrate the concept of lexical quality and the lexical quality hypothesis, the central ideas that link lower and higher level reading processes. Then we summarize experiments on simple meaning judgments.},
  copyright = {(c) 2012 APA, all rights reserved},
  isbn = {1-55798-879-X (Hardcover)},
  keywords = {*Ability Level,*Reading Ability,*Sentence Comprehension,*Theories,*Word Recognition,Ability Level,lexical access,Lexical Access,Reading Ability,Reading development,Reading Development,Sentence Comprehension,Theories,Word Meaning,Word Recognition},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/JRJMQZMP/2002-02486-004.html;/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/RVXDVV7H/index.html}
}

