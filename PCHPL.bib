  url = {http://journals.cambridge.org/article_S1366728906002847},
  urldate = {2015-04-16},
  abstract = {An eye tracking experiment examined how exposure to a second language (L2) influences sentence parsing in the first language. Forty-four monolingual Spanish speakers, 24 proficient Spanish–English bilinguals with limited immersion experience in the L2 environment and 20 proficient Spanish–English bilinguals with extensive L2 immersion experience read temporarily ambiguous constructions. The ambiguity concerned whether a relative clause (RC) that appeared after a complex noun phrase (NP) was interpreted as modifying the first or the second noun in the complex NP (El policía arrestó a la hermana del criado que estaba enferma desde hacía tiempo). The results showed that whereas the Spanish monolingual speakers and the Spanish–English bilinguals with limited exposure reliably attached the relative clause to the first noun, the Spanish–English bilingual with extensive exposure attached the relative to the second noun. Results are discussed in terms of models of sentence parsing most consistent with the findings.},
  file = {/Users/jmorris6/Zotero/storage/UCNSMYNM/displayAbstract.html}
}

@article{duyckSizeCrossLinguaMasked2004,
  title = {The {{Size}} of the {{Cross-Lingua Masked Phonological Priming Effect Does Not Depend}} on {{Second Language Proficiency}}},
  author = {Duyck, Wouter and Diependaele, Kevin and Drieghe, Denis and Brysbaert, Marc},
  date = {2004},
  journaltitle = {Experimental Psychology},
  volume = {51},
  number = {2},
  pages = {116--124},
  abstract = {Using a masked phonological priming paradigm, Brysbaert, Van Dyck, and Van de Poel (1999) showed that Dutch-French bilinguals perform better at identifying tachistoscopically presented L2 words (e.g., out lyes]) when those words are primed by L1 words or nonwords that are homophonic to the L2 target word according to the L1 graphemephoneme conversion rules (e.g., wie [who]). They noted that this priming effect was smaller for balanced bilinguals than for less proficient bilinguals, although the interaction failed to reach significance. Findings of Gollan, Forster, and Frost (1997) suggest that this could be attributed to a greater reliance on phonology in L2 reading, caused by a smaller proficiency in this language. However, in this study we show that the Dutch-French cross-lingual phonological priming effect is equally large for perfectly balanced and less proficient bilinguals. Our findings are in line with more recent work of Van Wijnendaele and Brysbaert (2002). (PsycINFO Database Record (c) 2005 APA, all rights reserved) (journal abstract).},
  keywords = {(18,[2720].,*Bilingualism,*Language,*Language Proficiency,*Phonology,*Semantic,*Semantic Priming,\&,Adulthood,Belgium.,Child,Decision,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older),Humans,Language,Lexical,Lexical Decision,Linguistics,Linguistics \& Language \& Speech [2720].,Male.,older),Perceptual Masking,Phonetics,Priming,Proficiency,Questionnaires,Speech,yrs},
  annotation = {Peer Reviewed Journal References . http://www.hhpub.com/journals/exppsy/index.html Journal Peer Reviewed Journal}
}

@article{duyckVisualWordRecognition2007,
  title = {Visual Word Recognition by Bilinguals in a Sentence Context: {{Evidence}} for Nonselective Lexical Access},
  shorttitle = {Visual Word Recognition by Bilinguals in a Sentence Context},
  author = {Duyck, Wouter and Assche, Eva Van and Drieghe, Denis and Hartsuiker, Robert J.},
  date = {2007},
  journaltitle = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
  volume = {33},
  number = {4},
  pages = {663--679},
  issn = {1939-1285(Electronic);0278-7393(Print)},
  doi = {10.1037/0278-7393.33.4.663},
  abstract = {Recent research on bilingualism has shown that lexical access in visual word recognition by bilinguals is not selective with respect to language. In the present study, the authors investigated language-independent lexical access in bilinguals reading sentences, which constitutes a strong unilingual linguistic context. In the first experiment, Dutch-English bilinguals performing a 2nd language (L2) lexical decision task were faster to recognize identical and nonidentical cognate words (e.g., banaan-banana) presented in isolation than control words. A second experiment replicated this effect when the same set of cognates was presented as the final words of low-constraint sentences. In a third experiment that used eyetracking, the authors showed that early target reading time measures also yield cognate facilitation but only for identical cognates. These results suggest that a sentence context may influence, but does not nullify, cross-lingual lexical interactions during early visual word recognition by bilinguals.},
  keywords = {*Bilingualism,*Lexical Access,*Sentences,*Word Recognition,Lexical Decision,Linguistics}
}

@article{duzelEventrelatedBrainPotential1997,
  title = {Event-Related Brain Potential Correlates of Two States of Conscious Awareness in Memory},
  author = {Duzel, E and Yonelinas, A P and Mangun, G R and Heinze, H J and Tulving, E},
  date = {1997-05},
  journaltitle = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {94},
  number = {11},
  pages = {5973--5978},
  issn = {0027-8424},
  abstract = {We report an event-related potential (ERP) experiment of human recognition memory that explored the relation between conscious awareness and electrophysiological activity of the brain. We recorded ERPs from healthy adults while they made "remember" and "know" recognition judgments about previously seen words. These two kinds of judgments reflect "autonoetic" and "noetic" awareness, respectively. The ERP effects differed between the two kinds of awareness while they were similar for "true" and "false" recognition. Noetic awareness was associated with a temporoparietal positivity in the N400 range (325-600 ms) and a late (600-1,000 ms) frontocentral negativity, whereas autonoetic awareness was associated with a widespread, late, bifrontal and left parietotemporal (600-1000 ms) positivity. In the very late (1,300-1, 900 ms) time window, a right frontal positivity was observed for both remember and know judgments of both true and false targets. These results provide physiological evidence for two types of conscious awareness in episodic memory retrieval.},
  keywords = {*Awareness/ph [Physiology],*Brain Mapping,*Brain/ph [Physiology],*Evoked Potentials/ph [Physiology],*Memory/ph [Physiology],Adult,Adult *Awareness/ph [Physiology] *Brain/ph [Physiology] *Brain Mapping *Evoked Potentials/ph [Physiology] Female Humans Male *Memory/ph [Physiology] Neurons/ph [Physiology] Reaction Time Research Support,Female,Humans,Male,Neurons/ph [Physiology],Non-U.S. Gov't Research Support,P.H.S. Time Factors,reaction time,{Research Support, Non-U.S. Gov't},{Research Support, U.S. Gov't, P.H.S.},Time Factors,U.S. Gov't}
}

@article{dykmanAuditoryERPAugmentationreduction1983,
  title = {Auditory {{ERP}} Augmentation-Reduction and Methylphenidate Dosage Needs in Attention and Reading Disordered Children},
  author = {Dykman, Roscoe A. and Holcomb, Phillip J. and Ackerman, Peggy T. and McCray, David S.},
  date = {1983},
  journaltitle = {Psychiatry Research},
  volume = {9},
  number = {3},
  pages = {255--269},
  abstract = {From their event-related potentials (ERPs) to tones of 4 intensity levels, 32 attention-disordered and/or reading-disabled males (mean age 8.5 yrs), who were recommended for a trial on methylphenidate, were classified as auditory augmenters or reducers. The augmenters were blindly titrated at significantly lower dosage levels than the reducers. The ameliorative effects of the drug, as assessed by teacher ratings, were more evident in hyperactive augmenters. Ss diagnosed either as hyperactive or hyperactive and reading disabled had steeper (or more augmenting) gradients than the nonhyperactive reading-disabled and attention-disordered Ss. Auditory ERP augmentation appears to be related to the clinical diagnosis of childhood hyperactivity, but auditory ERP augmenters have neither more augmenting RT gradients nor stronger or weaker nervous systems than reducers. Results suggest that reducers either require or tolerate higher dosages of methylphenidate than augmenters and that hyperactive augmenters derive greater clinical benefit from the stimulant than hyperactive reducers. (20 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(6-12,(birth-12,[3340].,*Attention,*Auditory,*Auditory Evoked Potentials,*Drug,*Drug Therapy,*Methylphenidate,*Reading,*Reading Disabilities,Age,Childhood,Clinical,Clinical Psychopharmacology [3340].,Disabilities,Disorders,Evoked,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Hyperkinesis,Potentials,Psychopharmacology,School,Therapy,yrs).},
  annotation = {Elsevier Scientific, US Empirical Study. Journal Article. English}
}

@article{dykmanAutonomicResponsivityVisual1982,
  title = {Autonomic Responsivity during Visual Search of Hyperactive and Reading-Disabled Children},
  author = {Dykman, Roscoe A. and Ackerman, Peggy T. and Oglesby, D. and Holcomb, Phillip J.},
  date = {1982},
  journaltitle = {Pavlovian Journal of Biological Science},
  volume = {17},
  number = {3},
  pages = {150--157},
  abstract = {Compared heart rate (HR) and skin conductance measures, recorded during a visual search task, for 10 hyperactive, 10 reading-disabled, 10 hyperactive reading-disabled, and 10 control males (aged 7.5-10 yrs). Basal autonomic levels did not statistically differentiate groups. In all groups, HR levels increased with task complexity, but more so on reward than on nonreward trials. In the intertrial interval, HR decelerated consistently when Ss were anticipating stimuli associated with reward; but when reward was uncertain, HR accelerated slightly or stayed the same. Control Ss exhibited anticipatory HR deceleration more consistently than did clinical Ss, especially the solely hyperactive ones. Skin conductance levels first decreased, then rose as task complexity increased, but the groups did not differ on this measure. A conditioning model is outlined to explain directional changes in HR. (31 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(6-12,(birth-12,[3290].,*Heart,*Heart Rate,*Rewards,*Skin,*Skin Resistance,*Task,*Task Complexity,*Visual,*Visual Search,\&,Age,Childhood,Complexity,Disabilities,Disorders,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Hyperkinesis,Physical,Physical \& Somatoform \& Psychogenic Disorders [329,Psychogenic,Rate,Reading,Reading Disabilities,Resistance,School,Search,Somatoform,yrs).},
  annotation = {Transaction Periodicals Consortium, US Journal Article. English}
}

@article{dykmanElectrocorticalFrequenciesHyperactive1982,
  title = {Electrocortical Frequencies in Hyperactive, Learning-Disabled, Mixed, and Normal Children},
  author = {Dykman, Roscoe A. and Holcomb, Phillip J. and Oglesby, D. and Ackerman, Peggy T.},
  date = {1982},
  journaltitle = {Biological Psychiatry},
  volume = {17},
  number = {6},
  pages = {675--685},
  abstract = {10 7.5-10 yr old males in each of four groups (hyperactive, learning disabled, hyperactive/learning disabled or mixed, and normal) were exposed to a complex visual search task. Ss had previously completed the WISC, the Wide Range Achievement Test, and the Gray Oral Reading Test. EEGs were recorded at central and parietal sites, 1-sec pre- and poststimulus onset, on 3 types of trials. The resulting wave forms, averaged over trials, were converted to the frequency domain and factored by principal components. Four components accounting for 87\% of the variance were varimax-rotated. ANOVA of the component scores revealed that Component 1 differentiated the four groups. This component had highest loadings in frequencies from 16 to 20 Hz and secondary loading in frequencies from 7 to 10 Hz. It is concluded that this multifrequency component is sensitive to parameters deficient in hyperactive and learning-disabled children. (21 ref) (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(6-12,(birth-12,[3290].,*Electroencephalography,*Hyperkinesis,*Learning,*Learning Disabilities,*Visual,*Visual Search,\&,Age,Childhood,Disabilities,Disorders,Human.,Human. Childhood (birth-12 yrs). School Age (6-12,Physical,Physical \& Somatoform \& Psychogenic Disorders [329,Psychogenic,School,Search,Somatoform,yrs).},
  annotation = {Elsevier Science, US Journal Article. English}
}

@article{dykmanPhysiologicalManifestationsLearning1983,
