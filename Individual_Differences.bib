  title = {Individual Differences in Verbal Working Memory Underlie a Tradeoff between Semantic and Structural Processing Difficulty during Language Comprehension: {{An ERP}} Investigation},
  shorttitle = {Individual Differences in Verbal Working Memory Underlie a Tradeoff between Semantic and Structural Processing Difficulty during Language Comprehension},
  author = {Kim, Albert E. and Oines, Leif and Miyake, Akira},
  year = {2018},
