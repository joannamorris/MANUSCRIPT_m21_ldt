@article{mccormickMorphologicalDecompositionLimited2009,
  ids = {mccormick_is_2009-1},
  title = {Is Morphological Decomposition Limited to Low Frequency Words},
  author = {McCormick, SF Samantha F and Brysbaert, Marc and Rastle, Kathleen},
  date = {2009},
  journaltitle = {The Quarterly Journal of Experimental Psychology},
  volume = {62},
  number = {9},
  pages = {1706--1715},
  issn = {1747-0226 1747-0218},
  doi = {10.1080/17470210902849991},
  url = {http://proxy2.hampshire.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&AuthType=ip&db=psyh&AN=2009-11532-004&site=ehost-live},
  abstract = {On the basis of data from masked priming experiments, it has been argued that an automatic process of decomposition is applied to all morphologically structured stimuli, irrespective of their lexical characteristics (Rastle, Davis, \& New, 2004). So far, this claim has been tested only with respect to low-frequency primes and non word primes. This is a limitation because some models of morphological processing postulate that only high-frequency complex words are recognized as whole forms. Thus, a more stringent test would be to determine whether high-frequency complex words also show evidence of masked priming. We report an experiment that compares masked-priming effects observed when the primes constitute morphologically structured non words (e.g., alarmer‚ÄìALARM), low-frequency words with a mean frequency of 2 per million (e.g., notional‚ÄìNOTION), and high-frequency words with a mean frequency of 60 per million (e.g., national‚ÄìNATION). These three conditions yielded significant and equivalent effects, lending strong support to the notion of a routine form of decomposition that is applied to all morphologically structured stimuli. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (journal abstract)},
  keywords = {(Language),(Phonetic,Access,automatic,automatic process,Characteristics,decomposition,Frequency,Lexical,Lexical Access,lexical characteristics,low,low frequency words,morphological,morphological decomposition,Morphology,Morphology (Language),Priming,process,Stimuli,structured,structured stimuli,Units),Word,Word Frequency,Words,Words (Phonetic Units)},
  file = {/Users/jmorris6/Zotero/storage/FBDFBUBZ/McCormick et al. - 2009 - Is morphological decomposition limited to low-freq.pdf;/Users/jmorris6/Zotero/storage/ZR3G7QU4/McCormick, Brysbaert, Rastle. 2009.pdf}
}

@article{mckinnonConstraintsMovementPhenomena1996,
  title = {Constraints on Movement Phenomena in Sentence Processing: {{Evidence}} from Event-Related Brain Potentials},
  author = {McKinnon, Richard and Osterhout, Lee},
  date = {1996},
  journaltitle = {Language \& Cognitive Processes},
  volume = {11},
  number = {5},
  pages = {495--523},
  abstract = {Event-related potentials (ERPs) were recorded from 13 scalp electrodes while 18-32-yr-old Ss read sentences, some of which contained a violation of constraints on constituent movement (subjacency or the empty category principle). In two experiments, the first word that indicated a violation elicited a widely distributed positive-going wave with an onset between 250 and 300 msec and a duration of several hundred milliseconds. This effect was similar to the positive shifts that have been observed in response to other types of syntactic violations (P600), but qualitatively distinct from the N400 response elicited by semantically/pragmatically anomalous words. These findings are taken as an indication that movement constraints can be applied during the earliest stages of sentence processing, perhaps in conjunction with the creation of phrase structure. We evaluate the hypothesis that the P600 is a general electrophysiological marker of syntactic anomaly. (PsycINFO Database Record (c) 2002 APA, all rights reserved).},
  keywords = {(18,(18-29,(30-39,[2340].,*Cognitive,*Cognitive Processes,*Cortical,*Cortical Evoked Potentials,*Sentence,*Sentence Comprehension,*Sentence Structure,\&,Adulthood,Cognitive,Cognitive Processes [2340].,Comprehension,Evoked,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older). Y,Male.,older).,Potentials,Processes,Structure,Thirties,Young,yrs,yrs).},
  annotation = {Taylor \& Francis, US Empirical Study. Journal Article. English}
}

@article{mckinnonMorphologicalDecompositionInvolving2003,
  title = {Morphological Decomposition Involving Non-Productive Morphemes: {{ERP}} Evidence.},
  author = {McKinnon, Richard and Allen, Mark and Osterhout, Lee},
  date = {2003-03},
  journaltitle = {NeuroReport: For Rapid Communication of Neuroscience Research},
  shortjournal = {NeuroReport: For Rapid Communication of Neuroscience Research},
  volume = {14},
  number = {6},
  pages = {883--886},
  issn = {1473-558X},
  url = {http://proxy2.hampshire.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&AuthType=ip&db=psyh&AN=2003-99199-014},
  abstract = {It is generally believed that readers decompose a complex word into its constituent morphemes only when those morphemes participate productively in word formation. Here we recorded eventrelated potentials (ERPs) to words (e.g. muffler, receive), nonwords containing no morphemes (e.g. flermuf), and non-words containing a prefix and a non-productive bound stem (e.g. in-ceive). Prior work has shown that pronounceable non-words elicit larger-amplitude N400 components than words. If readers treat non-words containing non-productive morphemes as unanalyzed wholes, then these non-words should elicit larger N400s than matched words. We report here, however, that bound-stem nonwords elicit a brain response highly similar to that elicited by real words. This finding suggests that morphological decomposition and representation extend to non-productive morphemes. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (journal abstract)},
  keywords = {(18,(Language),(Phonetic,[2720].,*Evoked,*Evoked Potentials,*Morphemes,*Pronunciation,*Words,*Words (Phonetic Units),\&,Adulthood,brain response,complex words,event-related potentials,Evoked Potentials,Female.,Human.,Human. Male. Female. Adulthood (18 yrs \& older).,Language,Linguistics,Linguistics \& Language \& Speech [2720].,Male.,Morphemes,morphological decomposition,Morphology,Morphology (Language),non-productive bound stem,non-words,older).,Potentials,prefix,pronounceable,Pronunciation,Reading,real words,Speech,Units),word formation,Words (Phonetic Units),yrs},
  file = {/Users/jmorris6/Zotero/storage/ZPT7AGZ9/McKinnon, Allen, Osterhout. 2003.pdf}
}

@article{mclaughlinNeuralCorrelatesSecondlanguage2004,
