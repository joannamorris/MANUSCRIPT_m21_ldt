@article{vanwitteloostuijnAssessingVisualStatistical2019,
  title = {Assessing {{Visual Statistical Learning}} in {{Early-School-Aged Children}}: {{The Usefulness}} of an {{Online Reaction Time Measure}}},
  shorttitle = {Assessing {{Visual Statistical Learning}} in {{Early-School-Aged Children}}},
  author = {family=Witteloostuijn, given=Merel, prefix=van, useprefix=true and Lammertink, Imme and Boersma, Paul and Wijnen, Frank and Rispens, Judith},
  date = {2019},
  journaltitle = {Frontiers in Psychology},
