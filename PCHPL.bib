@article{wagnerPrevalenceDyslexiaNew2020,
  title = {The {{Prevalence}} of {{Dyslexia}}: {{A New Approach}} to {{Its Estimation}}},
  shorttitle = {The {{Prevalence}} of {{Dyslexia}}},
  author = {Wagner, Richard K. and Zirps, Fotena A. and Edwards, Ashley A. and Wood, Sarah G. and Joyner, Rachel E. and Becker, Betsy J. and Liu, Guangyun and Beal, Bethany},
