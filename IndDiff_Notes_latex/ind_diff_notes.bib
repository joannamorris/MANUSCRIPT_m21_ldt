@article{dehaeneNeuralCodeWritten2005,
  ids = {dehaene_neural_2005-2},
  title = {The Neural Code for Written Words: A Proposal},
  shorttitle = {The Neural Code for Written Words},
  author = {Dehaene, Stanislas and Cohen, Laurent and Sigman, Mariano and Vinckier, Fabien},
  year = {2005},
  month = jul,
  journal = {Trends in Cognitive Sciences},
  volume = {9},
  number = {7},
  pages = {335--341},
  issn = {1364-6613},
  doi = {10.1016/j.tics.2005.05.004},
  urldate = {2014-10-27},
  abstract = {How is reading, a cultural invention, coded by neural populations in the human brain? The neural code for written words must be abstract, because we can recognize words regardless of their location, font and size. Yet it must also be exquisitely sensitive to letter identity and letter order. Most existing coding schemes are insufficiently invariant or incompatible with the constraints of the visual system. We propose a tentative neuronal model according to which part of the occipito-temporal `what' pathway is tuned to writing and forms a hierarchy of local combination detectors sensitive to increasingly larger fragments of words. Our proposal can explain why the detection of `open bigrams' (ordered pairs of letters) constitutes an important stage in visual word recognition.},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/FI3PKIVU/S1364661305001439.html}
}

@article{dejongMorphologicalFamilySize2000,
  title = {The Morphological Family Size Effect and Morphology},
  author = {{de Jong}, Nivja H. and Schreuder, Robert and Baayen, Harald},
  year = {2000},
  month = aug,
  journal = {Language and Cognitive Processes},
  volume = {15},
  number = {4-5},
  pages = {329--365},
  issn = {1464-0732},
  doi = {10.1080/01690960050119625},
  abstract = {Addresses the role of a new factor in visual word recognition, the Family size effect, for uninflected and inflected words in Dutch. Exp 1 compares the Family size effect for simplex nouns with the effect for simplex verbs in order to ascertain whether the word category of a simplex word affects the extent to which its family members are activated in the mental lexicon. Exp 2 shifts attention from monomorphemic words to complex words. Exp 3 addresses the question to what extent the presence or absence of an inflectional suffix affects the activation of the family members. Exp 4 explored the Family size effect for perfect participles. Ss were 198 native Dutch speakers. Results from Exp 1 found a similar effect of Family size for verbs without a nominal conversion alternant as for nouns without a verbal conversion alternant. No significant difference could be observed in the response latencies in Exp 2. Exp 3 showed that the Family size effect for complex words has a genuine morphological component independent of a semantic component. Exp 4 revealed an effect of Family size on both kinds of participles. The present study makes clear that the immediate morphological context in which a monomorphemic verb appears mediates the Family size effect. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  keywords = {(18,(Language),(Phonetic,[2720].,*Inflection,*Language,*Morphology,*Morphology (Language),*Visual,*Visual Perception,*Word,*Word Recognition,\&,Adulthood,Human.,Human. Adulthood (18 yrs \& older).,Inflection,Language,Linguistics,Linguistics \& Language \& Speech [2720].,Morphology (Language),older).,Perception,Recognition,Speech,Units),visual perception,{visual word recognition \& Family size effect \& uninflected vs inflected words \& morphology, native Dutch speakers},Word Recognition,WORD recognition,Words,Words (Phonetic Units),yrs}
}

@article{dunabeitiaRevisitingLetterTranspositions2014,
  title = {Revisiting Letter Transpositions within and across Morphemic Boundaries},
  author = {Du{\~n}abeitia, Jon Andoni and Perea, Manuel and Carreiras, Manuel},
  year = {2014},
  month = mar,
  journal = {Psychonomic Bulletin \& Review},
  volume = {21},
  number = {6},
  pages = {1557--1575},
  issn = {1069-9384, 1531-5320},
  doi = {10.3758/s13423-014-0609-2},
  urldate = {2016-09-04},
  abstract = {Early morphological decomposition of complex words has been supported by evidence showing that the magnitude of masked transposed-letter (TL) priming effects is greater for within-morpheme transpositions than for between-morpheme transpositions. However, these findings have lately been called into question, and a recent article by S\'anchez-Guti\'errez and Rastle (Psychonomic Bulletin \& Review, 20, 988\textendash 996, 2013) suggested that the above-mentioned interaction could have been the consequence of a false positive (i.e., a Type I error). Considering recent evidence showing that morpho-orthographic interactions are highly sensitive to individual differences in reading skills, we explored whether participants' averaged reading speeds were responsible for modulating the size of within- versus between-morpheme TL priming effects. A large-scale lexical decision experiment with a set of 420 suffixed Spanish words (N = 80 participants) was run using the masked-priming technique. The results revealed that individual differences modulated the magnitude of the masked TL priming effect between morphemes: Faster readers (but not slower readers) yielded greater TL priming for within- than for between-morpheme transpositions. The present data help reconcile previous divergent data by showing that faster readers revealed a morpho-orthographic interaction, whereas slower readers may rely more on a morphological-processing strategy that is not sensitive to morpho-orthographic interactions.},
  langid = {english},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/4DN7VNN4/s13423-014-0609-2.html}
}

@article{dunabeitiaTransposedletterSimilarityEffects2007,
  title = {Do Transposed-Letter Similarity Effects Occur at a Morpheme Level? {{Evidence}} for Morpho-Orthographic Decomposition},
  shorttitle = {Do Transposed-Letter Similarity Effects Occur at a Morpheme Level?},
  author = {Du{\~n}abeitia, Jon Andoni and Perea, Manuel and Carreiras, Manuel},
  year = {2007},
  month = dec,
  journal = {Cognition},
  volume = {105},
  number = {3},
  pages = {691--703},
  issn = {0010-0277},
  doi = {10.1016/j.cognition.2006.12.001},
  urldate = {2016-08-12},
  abstract = {When does morphological decomposition occur in visual word recognition? An increasing body of evidence suggests the presence of early morphological processing. The present work investigates this issue via an orthographic similarity manipulation. Three masked priming lexical decision experiments were conducted to examine the transposed-letter similarity effect (e.g., jugde facilitates JUDGE more than the control jupbe) in polymorphemic and monomorphemic words. If morphological decomposition occurs at early stages of visual word recognition, we would expect an interaction with transposed-letter effects. Experiment 1 was carried out in Basque, which is an agglutinative language. The nonword primes were created by transposing two letters that either crossed the morphological boundaries of suffixes or did not. Results showed a transposed-letter effect for non-affixed words, whereas there were no signs of a transposed-letter effect across morpheme boundaries for affixed words. In Experiment 2, this issue was revisited in a non-agglutinative language (Spanish), with prefixed and suffixed word pairs. Again, results showed a significant transposed-letter effect for non-affixed words, whereas there were no signs of a transposed-letter effect across morpheme boundaries for affixed words (both prefixed words and suffixed words). Experiment 3 replicated the previous findings, and also revealed that, for polymorphemic words, transposed-letter priming effects occurred for within-morpheme transpositions. Taken together, these findings support the view that morphological decomposition operates at an early stage of visual word recognition.},
  keywords = {Letter transpositions,lexical access,Morphology,Visual word recognition},
  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/6986S6II/S0010027706002447.html}
}

