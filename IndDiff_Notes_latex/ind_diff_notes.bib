  shorttitle = {Assessing {{Visual Statistical Learning}} in {{Early-School-Aged Children}}},
  author = {{van Witteloostuijn}, Merel and Lammertink, Imme and Boersma, Paul and Wijnen, Frank and Rispens, Judith},
  year = {2019},
