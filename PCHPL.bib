@article{pothUltrahighTemporalResolution2018,
  title = {Ultrahigh Temporal Resolution of Visual Presentation Using Gaming Monitors and {{G-Sync}}},
  author = {Poth, Christian H. and Foerster, Rebecca M. and Behler, Christian and Schwanecke, Ulrich and Schneider, Werner X. and Botsch, Mario},
  date = {2018-02-01},
