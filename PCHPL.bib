@article{goodmanInseparabilityGrammarLexicon1997,
  ids = {goodman_inseparability_1997-1},
  title = {On the {{Inseparability}} of {{Grammar}} and the {{Lexicon}}: {{Evidence}} from {{Acquisition}}, {{Aphasia}} and {{Real-time Processing}}},
  shorttitle = {On the {{Inseparability}} of {{Grammar}} and the {{Lexicon}}},
  author = {Goodman, Elizabeth Bates Judith C.},
  date = {1997-10-01},
  journaltitle = {Language and Cognitive Processes},
