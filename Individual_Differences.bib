@article{tannerERPsRevealIndividual2014,
  ids = {tanner_erps_2014-1},
  title = {{{ERPs}} Reveal Individual Differences in Morphosyntactic Processing},
  author = {Tanner, Darren and Van Hell, Janet G.},
  year = {2014},
