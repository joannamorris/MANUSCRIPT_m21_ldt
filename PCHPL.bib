  title = {Stem Homograph Inhibition and Stem Allomorphy: {{Representing}} and Processing Inflected Forms in a Multilevel Lexical System},
  author = {Allen, Mark and Badecker, William},
  date = {1999},
  journaltitle = {Journal of Memory \& Language},
  volume = {41},
