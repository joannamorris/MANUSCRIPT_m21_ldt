@article{engelmannHowInputShapes2019,
  title = {How the Input Shapes the Acquisition of Verb Morphology: {{Elicited}} Production and Computational Modelling in Two Highly Inflected Languages},
  shorttitle = {How the Input Shapes the Acquisition of Verb Morphology},
