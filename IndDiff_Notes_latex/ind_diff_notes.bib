  file = {/Users/jmorris/Library/CloudStorage/GoogleDrive-jamCCS@hampshire.edu/My Drive/PROVIDENCE COLLEGE/Zotero Data Directory/storage/IE5A57MQ/Goldwater et al. - 2006 - Contextual dependencies in unsupervised word segme.pdf}
}

@article{graingerWatchingWordGo2009,
  title = {Watching the Word Go by: {{On}} the Time-Course of Component Processes in Visual Word Recognition},
  author = {Grainger, Jonathan and Holcomb, Philip J.},
