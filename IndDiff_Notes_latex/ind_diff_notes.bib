@article{nation_formmeaning_2009,
  title = {Form\textendash{}Meaning Links in the Development of Visual Word Recognition},
  volume = {364},
  copyright = {\textcopyright{} 2009 The Royal Society},
  issn = {0962-8436, 1471-2970},
  abstract = {Learning to read takes time and it requires explicit instruction. Three decades of research has taught us a good deal about how children learn about the links between orthography and phonology during word reading development. However, we have learned less about the links that children build between orthographic form and meaning. This is surprising given that the goal of reading development must be for children to develop an orthographic system that allows meanings to be accessed quickly, reliably and efficiently from orthography. This review considers whether meaning-related information is used when children read words aloud, and asks what we know about how and when children make connections between form and meaning during the course of reading development.},
  language = {en},
  number = {1536},
  journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
  doi = {10.1098/rstb.2009.0119},
  author = {Nation, Kate},
  month = dec,
  year = {2009},
  pages = {3665-3674},
  file = {/Users/jmorris/Documents/Zotero Data Directory/storage/USWUHA3B/Nation - 2009 - Form–meaning links in the development of visual wo.pdf;/Users/jmorris/Documents/Zotero Data Directory/storage/H2QWN65B/Nation - 2009 - Form–meaning links in the development of visual wo.html},
  pmid = {19933139},
  note = {Learning to read takes time and it requires explicit instruction. Three decades of research has taught us a good deal about how children learn about the links between orthography and phonology during word reading development. However, we have learned less about the links that children build between orthographic form and meaning. This is surprising given that the goal of reading development must be for children to develop an orthographic system that allows meanings to be accessed quickly, reliably and efficiently from orthography. This review considers whether meaning-related information is used when children read words aloud, and asks what we know about how and when children make connections between form and meaning during the course of reading development.}
