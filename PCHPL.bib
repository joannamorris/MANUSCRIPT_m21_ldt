@article{hasenackerDoesMorphologicalStructure2021,
  title = {Does Morphological Structure Modulate Access to Embedded Word Meaning in Child Readers?},
  author = {Hasenäcker, Jana and Solaja, Olga and Crepaldi, Davide},
  date = {2021-10-01},
  journaltitle = {Memory \& Cognition},
  shortjournal = {Mem Cogn},
  volume = {49},
